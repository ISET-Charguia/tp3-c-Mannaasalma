#include <stdio.h>
#include <stdlib.h>

int main()
{
    int tva ;
    double pnet,pbrut;
    printf ("entree valeur ");
    scanf(" %d",&tva);
     printf ("entree valeur ");
    scanf(" %f",&pnet);
    pbrut= pnet + pnet * tva/100;
    printf(" pbrut =%d " , pbrut);
    return 0;
}
