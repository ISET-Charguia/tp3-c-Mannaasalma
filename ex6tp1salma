#include <stdio.h>
#include <stdlib.h>

int main()
{

   int a,b,c,aux;

   printf("entre valeur a");
   scanf("%d",&a);
    printf("entre valeur b");
   scanf("%d",&b);
    printf("entre valeur c");
   scanf("%d",&c);
   printf("avant permutation \n ");
   printf("a=%d  b=%d   c=%d  aux=%d" , a ,b,c,aux);
   aux=a;
   a=b;
   b=c;
   c=aux;
   printf("apres prmutur \n ");
   printf("a=%d b=%d c=%d aux =%d", a,b,c,aux );

   return 0 ;
}
