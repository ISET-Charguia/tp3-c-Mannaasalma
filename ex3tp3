#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n, i ,j ;
    int cpt=0;


        printf("entre valeur n");
        scanf("%d",&n);



    for (i=1  ; i<= n; i++)
    {
        for (j=2  ; j< (i / 2) ; j++)
        {
            if(i % j==0)
            {
                cpt=cpt+1;
            }
        }
    }

    if(cpt ==0)
    {
        printf (" i est permir ");
    }
    else
    {
        printf (" i est non  permir ");
    }
    return 0;

}


